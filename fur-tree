import java.util.Scanner;

public class Main {
    public static void main(String[] args){

        int height = 10;
        for (int i=1; i<height; i++) {

           for (int spase=1; spase<=height-i; spase++) {
               System.out.print(" ");

           }

           for (int star=1; star<=i*2-1; star++) {
               System.out.print("*");
           }

           System.out.println();
        }
    }
}
